'use strict';


var baseParser = require('./base').parser;


// nam base parser object (to be extended by probe-specific parsers)
var namParser = Object.create(baseParser);


// Extract data from request body
// Base format :
//{
//  "idTest": idTest,
//  "type": type,
//  "timestamp": timestamp,
//  "hostSource": IP_hostSource,
//  "hostDestination": IP_hostDestination,
//  "error": error,
//  "result": data
//}
//      
namParser.parseRequest = function() {
    var entityData = {};
    entityData.data = this.request.body;
    entityData.perfData += this.request.body;

    return entityData;
    
};


exports.parser = namParser;
