<script type="text/javascript">
$("#myTab .dropdown-menu a").click( function(e){
    
    var source = $(this).data("source");
    
    if( source )
    {
        $('#myTab a[href="#' + source + '"]').tab('show');
    }
    
    return false;
});

</script>

